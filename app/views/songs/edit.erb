<div class='container' style="padding-top: 20px;">
  <div class='row'>
    <div class='span8'>
      <div class="page-header">
      </div>
      <table class='table table-striped'>
        <tr>

<form action="/songs/<%= @song.id %>/edit" method="POST" >
    Song Name: <input type="text" name="song[title]" value="<%= @song.title %>">
    <br>
    Artist: <input type="text" name="song[artist]" value="<%= @song.artist %>">
    <br/>
    Album: <input type="text" name="song[album]" value="<%= @song.album %>">
    <br/>
    Genre: <input type="text" name="song[genre]" value="<%= @song.genre %>">
    <br/>
    Length: <input type="text" name="song[length]" value="<%= @song.length %>">
    <br/>
    <input type="submit" value="Save Changes">
</form>

      </table>
    </div>
  </div>
</div>
